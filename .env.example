TON_API_KEY=your_toncenter_api_key_here

# --- SWAP & FEES CONFIGURATION ---
# آدرس کیف‌پولی که کارمزد تراکنش‌ها (Fee) به آن واریز می‌شود
PLATFORM_FEE_ADDRESS=0xYourCentralFeeAddressHere
# درصد کارمزد اپلیکیشن از هر سوآپ (مثلاً 0.003 معادل 0.3%)
SWAP_FEE_PERCENT=0.003

